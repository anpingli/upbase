ansible all -m shell ssh-copy-id root@$i
 done
}

sshkeycopy
echo "shrink system to free space"
ansible all -m copy -a "src=tools/ dest=/root/bin/" -i host
ansible all -m shell -a ">/var/log/messages" -i host
ansible all -m shell -a "rm -rf /var/log/messages-*" -i host
ansible all -m shell -a "/root/bin/shrink" -i host

